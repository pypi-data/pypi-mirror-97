{
  "version": 1,
  "resources": {
    "HostedZone": {
      "arn": {
        "formatString": "arn:{partition}:{service}:::{id}",
        "replace": [":/", ":"]
      }
    },
    "HealthCheck": {
      "arn": {
        "formatString": "arn:{partition}:{service}:::healthcheck/{id}"
      }
    }
  }
}