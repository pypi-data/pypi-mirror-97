<cfdi:Addenda 
    xmlns:ahmsa="http://www.ahmsa.com/xsd/AddendaAHM1"
    xsi:schemaLocation="http://www.ahmsa.com/xsd/AddendaAHM1
    http://www.ahmsa.com/xsd/AddendaAHM1/AddendaAHM.xsd">
            <ahmsa:AddendaAHM Version="1.0">
            <ahmsa:Documento 
                Tipo="{{ tipo_documento }}" 
                Clase="{{ clase_documento }}"
            >
            <ahmsa:Encabezado 
                NumSociedad="{{ encabezado_num_sociedad }}" 
                NumDivision="{{ encabezado_num_dicision }}" 
                NumProveedor="{{ encabezado_num_proveedor }}" 
                Correo="{{ encabezado_correo }}" 
                Moneda="{{ moneda_nombre }}"
            />

            <ahmsa:Detalle>
                <ahmsa:Pedido Num="{{ num_pedido }}">
                <ahmsa:Recepcion></ahmsa:Recepcion>
                </ahmsa:Pedido>
                <ahmsa:HojaServicio Num="{{ num_hoja_servicio }}"/>
                <ahmsa:Transporte Num="{{ num_transporte }}"/>
                <ahmsa:CtaxPag Num="{{ num_ctax_pag }}" Ejercicio="{{ ejercicio_ctax_pag }}"/>
                <ahmsa:Liquidacion FechaInicio="{{ fecha_inicio_liquidacion }}" FechaFin="{{ fecha_fin_liquidacion }}"/>
            </ahmsa:Detalle>
            <ahmsa:Anexos>
                <ahmsa:Anexo></ahmsa:Anexo>
            </ahmsa:Anexos>            
         </ahmsa:Documento>
    </ahmsa:AddendaAHM>
</cfdi:Addenda>