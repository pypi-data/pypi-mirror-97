{% load cfdi_tags %}
<cfdi:Addenda>
	<pm:Addenda_Pemex xsi:schemaLocation="http://pemex.com/facturaelectronica/addenda/v2 https://pemex.reachcore.com/schemas/addenda-pemex-v2.xsd" xmlns:pm="http://pemex.com/facturaelectronica/addenda/v2">

		<pm:N_ACREEDOR>{{ no_acreedor }}</pm:N_ACREEDOR>

		<pm:EJERCICIO>{{ year }}</pm:EJERCICIO>

		<pm:CLAVE_TRANSP>{{ clave_transp }}</pm:CLAVE_TRANSP>

		<pm:A_RELACION>{{ year }}</pm:A_RELACION>

		<pm:ID_ANALITICO>{{ id_analitico }}</pm:ID_ANALITICO>

		<pm:TIPO_PRODUCTO>{{ tipo_producto }}</pm:TIPO_PRODUCTO>

		<pm:CEDULA>{{ cedula }}</pm:CEDULA>

		<pm:CONTRATO_SIIC>{{ contrato }}</pm:CONTRATO_SIIC>

		<pm:ANALITICO>{{ analitico }}</pm:ANALITICO>

	</pm:Addenda_Pemex>

</cfdi:Addenda>

