{% load cfdi_tags %}
<cce11:ComercioExterior 
    {% for xmlns in complemento.xmlns_list %}
        {{ xmlns|safe }}
    {% endfor %}
    Version="1.1" 
    {% set_field 'CertificadoOrigen' complemento.CertificadoOrigen %}
    {% set_field 'ClaveDePedimento' complemento.ClaveDePedimento %}
    {% set_field 'NumCertificadoOrigen' complemento.NumCertificadoOrigen %}
    {% set_field 'NumeroExportadorConfiable' complemento.NumeroExportadorConfiable %}
    {% set_field 'Incoterm' complemento.Incoterm %}
    {% set_field 'Observaciones' complemento.Observaciones %}
    {% set_field 'Subdivision' complemento.Subdivision %}
    {% set_field 'TipoCambioUSD' complemento.TipoCambioUSD %}
    {% set_field 'TipoOperacion' complemento.TipoOperacion %}
    {% set_field 'TotalUSD' complemento.TotalUSD %}
    >
    
    <cce11:Emisor {% set_field 'Curp' complemento.Emisor.Curp %}>
        <cce11:Domicilio 
            {% set_field 'Calle' complemento.Emisor.Calle %}
            {% set_field 'NumeroExterior' complemento.Emisor.NumeroExterior %}
            {% set_field 'NumeroInterior' complemento.Emisor.NumeroInterior %}
            {% set_field 'Colonia' complemento.Emisor.Colonia %}
            {% set_field 'Localidad' complemento.Emisor.Localidad %}
            {% set_field 'Referencia' complemento.Emisor.Referencia %}
            {% set_field 'Municipio' complemento.Emisor.Municipio %}
            {% set_field 'Estado' complemento.Emisor.Estado %}
            {% set_field 'Pais' complemento.Emisor.Pais %}
            {% set_field 'CodigoPostal' complemento.Emisor.CodigoPostal %}
        />
    </cce11:Emisor>

    <cce11:Receptor>
        <cce11:Domicilio 
            {% set_field 'Calle' complemento.Receptor.Calle %}
            {% set_field 'NumeroExterior' complemento.Receptor.NumeroExterior %}
            {% set_field 'NumeroInterior' complemento.Receptor.NumeroInterior %}
            {% set_field 'Colonia' complemento.Receptor.Colonia %}
            {% set_field 'Localidad' complemento.Receptor.Localidad %}
            {% set_field 'Referencia' complemento.Receptor.Referencia %}
            {% set_field 'Municipio' complemento.Receptor.Municipio %}
            {% set_field 'Estado' complemento.Receptor.Estado %}
            {% set_field 'Pais' complemento.Receptor.Pais %}
            {% set_field 'CodigoPostal' complemento.Receptor.CodigoPostal %}
        />
    </cce11:Receptor>

    <cce11:Mercancias>
        {% for mercancia in complemento.Mercancias %}
            <cce11:Mercancia
                {% set_field 'CantidadAduana' mercancia.CantidadAduana %}
                {% set_field 'FraccionArancelaria' mercancia.FraccionArancelaria %}
                {% set_field 'UnidadAduana' mercancia.UnidadAduana %}
                {% set_field 'NoIdentificacion' mercancia.NoIdentificacion %}
                {% set_field 'ValorUnitarioAduana' mercancia.ValorUnitarioAduana %}
                {% set_field 'ValorDolares' mercancia.ValorDolares %}
            
            {% if mercancia.DescripcionesEspecificas %}
                >
                {% for descesp in mercancia.descripciones_especificas %}
                    <cce11:DescripcionesEspecificas 
                        {% set_field 'Marca' descesp.Marca %}
                        {% set_field 'Modelo' descesp.Modelo %}
                        {% set_field 'SubModelo' descesp.SubModelo %}
                        {% set_field 'NumeroSerie' descesp.NumeroSerie %}
                    />
                 </cce11:Mercancia>
            {% else %}
               />
            {% endif %}

    </cce11:Mercancias>
</cce11:ComercioExterior>