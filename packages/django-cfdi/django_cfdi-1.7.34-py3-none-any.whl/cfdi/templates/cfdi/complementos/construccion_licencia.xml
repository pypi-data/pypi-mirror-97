{% load cfdi_tags %}
<servicioparcial:parcialesconstruccion 
    {% for xmlns in complemento.xmlns_list %}
        {{ xmlns|safe }}
    {% endfor %}
    Version="1.0" 
    NumPerLicoAut="{{ complemento.licencia|escape }}"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="{{ complemento.schemaLocation }}"
    >
    <servicioparcial:Inmueble 
        Calle="{{ complemento.calle }}"
        {% set_field 'NoExterior' complemento.no_exterior %}
        {% set_field 'NoInterior' complemento.no_interior %}
        {% set_field 'Colonia' complemento.colonia %}
        {% set_field 'Localidad' complemento.localidad %}
        {% set_field 'Referencia' complemento.referencia %}
        Municipio="{{ complemento.municipio|escape }}" 
        Estado="{{ complemento.estado|escape }}" 
        CodigoPostal="{{ complemento.codigo_postal|escape }}" 
    /> 
</servicioparcial:parcialesconstruccion>
