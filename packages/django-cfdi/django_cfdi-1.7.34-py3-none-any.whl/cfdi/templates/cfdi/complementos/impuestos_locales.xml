<implocal:ImpuestosLocales 
    {% for xmlns in complemento.xmlns_list %}
        {{ xmlns|safe }}
    {% endfor %}
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="{{ complemento.schemaLocation }}" 
    version="1.0" 
    TotaldeRetenciones="{{ complemento.total_retenciones_locales }}" 
    TotaldeTraslados="{{ complemento.total_traslados_locales }}">
    {% for rl in complemento.retenciones_locales %}
        <implocal:RetencionesLocales 
            ImpLocRetenido="{{ rl.nombre }}" 
            TasadeRetencion="{{ rl.tasa|stringformat:".2f" }}" 
            Importe="{{ rl.monto|stringformat:".2f" }}"
        />
    {% endfor %}

    {% for tl in complemento.traslados_locales %}
        <implocal:TrasladosLocales 
            ImpLocTrasladado="{{ tl.nombre }}" 
            TasadeTraslado="{{ tl.tasa|stringformat:".2f" }}" 
            Importe="{{ tl.monto|stringformat:".2f" }}"
        />
    {% endfor %}
</implocal:ImpuestosLocales>