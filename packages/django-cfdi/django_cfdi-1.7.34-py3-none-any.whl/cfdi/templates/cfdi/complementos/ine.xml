{% load cfdi_tags %}
<ine:INE 
    {% for xmlns in complemento.xmlns_list %}
        {{ xmlns|safe }}
    {% endfor %}
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="{{ complemento.schemaLocation }}" 
    Version="1.1" 
    {% if complemento.tipo_proceso == "Ordinario" %}
        TipoComite="{{ complemento.tipo_comite|escape }}"
    {% endif %}
    TipoProceso="{{ complemento.tipo_proceso }}"
    > 
    {% if complemento.clave_entidad %}
        <ine:Entidad 
            {% if complemento.tipo_proceso != "Ordinario" %}
                Ambito="{{ complemento.get_ambito_entidad }}"
            {% endif %}
            ClaveEntidad="{{ complemento.clave_entidad|escape }}"
            >
            <ine:Contabilidad IdContabilidad="{{ complemento.id_contabilidad|escape }}" />
        </ine:Entidad>
    {% endif %}
    
</ine:INE>