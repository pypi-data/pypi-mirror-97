<leyendasFisc:LeyendasFiscales 
    {% for xmlns in complemento.xmlns_list %}
        {{ xmlns|safe }}
    {% endfor %}
    xsi:schemaLocation="{{ complemento.schemaLocation }}"
    version="1.0" 
    >

    {% for leyenda_fisc in complemento.leyendasFiscales %}
        <leyendasFisc:Leyenda 
            disposicionFiscal="{{ leyenda_fisc.disposicionFiscal }}" 
            norma="{{ leyenda_fisc.norma }}" 
            textoLeyenda="{{ leyenda_fisc.textoLeyenda }}" 
        />
    {% endfor %}
        
</leyendasFisc:LeyendasFiscales>