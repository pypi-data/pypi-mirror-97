[TimeLoop]
DtInitial = 1 # [s]
TEnd = 3e4 # [s]
MaxTimeStepSize = 1e3

[Grid]
LowerLeft = 0
UpperRight = 20
Cells = 80

[Problem]
Name = 1pnifv_convection # name passed to the output routines
OutputInterval = 5 # every 5th timestep an output file is written
DarcyVelocity = 1e-4 # [m/s] inflow at the left boundary
EnableGravity = false # Disable gravity

[Vtk]
AddVelocity = true # enable velocity output

[MpfaTest.Vtk]
AddVelocity = false # enable velocity output

[Component]
SolidDensity = 2700
SolidThermalConductivity = 2.8
SolidHeatCapacity = 790
