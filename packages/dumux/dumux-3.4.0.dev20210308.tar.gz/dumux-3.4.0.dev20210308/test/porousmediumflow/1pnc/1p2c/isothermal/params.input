[TimeLoop]
DtInitial = 1 # [s]
TEnd = 100 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 20 2

[Problem]
Name = outflowtest # name passed to the output routines
EnableGravity = 0 # disable gravity
UseNitscheTypeBc = true

[Newton]
UseLineSearch = true

[Vtk]
AddVelocity = 1 # enable velocity output
