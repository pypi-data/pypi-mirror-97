{
  "name": "labbox-ephys-widgets-jp",
  "version": "0.1.1",
  "description": "Labbox-ephys jupyter widgets",
  "author": "Jeremy Magland",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/flatironinstitute/labbox-ephys-widgets-jp.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../labbox_ephys_widgets_jp/labextension/ && rimraf ../labbox_ephys_widgets_jp/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "tsc && webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "tsc && webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "rimraf": "^2.6.1",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "webpack": "^5"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "lodash": "^4.17.21"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../labbox_ephys_widgets_jp/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
