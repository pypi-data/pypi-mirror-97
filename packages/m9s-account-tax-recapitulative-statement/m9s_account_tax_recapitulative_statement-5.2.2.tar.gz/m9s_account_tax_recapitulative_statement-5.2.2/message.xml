<tryton>
    <data group="1">
        <record model="ir.message" id="same_vat_code_required">
            <field name="text">All child taxes must have the same setting for vat code required.\nTaxes "%(first)s" and "%(second)s" have different settings.</field>
        </record>
        <record model="ir.message" id="invalid_vat">
            <field name="text">Invalid VAT number!</field>
        </record>
        <record model="ir.message" id="missing_party_move_line">
            <field name="text">Please set a party on move line.</field>
        </record>
        <record model="ir.message" id="missing_party_vat_code">
            <field name="text">This tax requires a vat code set for the party "%(party)s%".</field>
        </record>
        <record model="ir.message" id="missing_line_vat_code">
            <field name="text">Vat Code missing on tax line %(line)s, that uses a tax requiring a vat code.</field>
        </record>
    </data>
</tryton>
