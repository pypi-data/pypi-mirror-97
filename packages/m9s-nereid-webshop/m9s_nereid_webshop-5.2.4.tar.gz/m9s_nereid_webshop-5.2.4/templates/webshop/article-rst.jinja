{% extends 'article.jinja' %}

{% block main %}
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>{{ article.title }}</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
    <p>{{ article.content|rst|safe }}</p>
    </div>
  </div>
</div>

{% endblock main %}
