<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/Malden.MetricsServer.Service.Process.Data" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/Malden.MetricsServer.Service.Process.Data" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="HeaderlessSettingData">
    <xs:sequence>
      <xs:element minOccurs="0" name="Channel" type="xs:int" />
      <xs:element minOccurs="0" name="DegradedFormat" type="tns:HeaderlessFormat" />
      <xs:element minOccurs="0" name="DegradedSampleRate" type="xs:int" />
      <xs:element minOccurs="0" name="ReferenceFormat" type="tns:HeaderlessFormat" />
      <xs:element minOccurs="0" name="ReferenceSampleRate" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="HeaderlessSettingData" nillable="true" type="tns:HeaderlessSettingData" />
  <xs:simpleType name="HeaderlessFormat">
    <xs:restriction base="xs:string">
      <xs:enumeration value="LittleEndian" />
      <xs:enumeration value="BigEndian" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="HeaderlessFormat" nillable="true" type="tns:HeaderlessFormat" />
</xs:schema>