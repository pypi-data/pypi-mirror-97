print("Length =", len([1, 2, 3]))
print("Cleared =", listClear([1, 2, 3]))
print("Sorted =", listSort(["one", "two", "three", "four"]))
print("Reversed =", listReverse(["one", "two", "three", "four"]))
print("Appended =", listAppend(["one", "two", "three", "four"], "five"))
print("Removed =", listRemove(["one", "two", "three", "four"], "two"))

