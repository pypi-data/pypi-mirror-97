data saveListTable {
    key1
    key3
}

myList = listGenerator(%listKind)

isEqual(%insertType,"as_is") {
    putlist myList => saveListTable
}

isEqual(%insertType,"selInv") {
    putlist myList(key02 key01) => saveListTable
}

isEqual(%insertType,"selColInv") {
    putlist myList(key02 key01) => saveListTable(key3 key1)
}

isEqual(%insertType,"colInv") {
    putlist myList => saveListTable(key3 key1)
}

isEqual(%insertType,"oneSel") {
    putlist myList(key02) => saveListTable(key1)
}

isEqual(%insertType,"oneSelConst") {
    putlist myList(key02) "key00" => saveListTable
}

iterate (saveListTable, "t") {
    print("Saved key1: {t.key1}, key3: {t.key3}")
}
