function customSleep(n) {
    print("Begin sleep {n}")
    sleep(ms=n)
    send(n)
    print("End sleep {n}")
}

function readNumber(n) {
    print("readNumber {n}")
}

customSleep(3) => waitForConsumerQueue
customSleep(2) => waitForConsumerQueue
customSleep(1) => waitForConsumerQueue

readNumber(:waitForConsumerQueue)

print("END")
