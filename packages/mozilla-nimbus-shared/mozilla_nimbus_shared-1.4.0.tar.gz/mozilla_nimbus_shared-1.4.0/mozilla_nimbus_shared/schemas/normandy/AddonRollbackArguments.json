{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/AddonRollbackArguments",
  "definitions": {
    "AddonRollbackArguments": {
      "type": "object",
      "properties": {
        "rolloutSlug": {
          "type": "string",
          "description": "Unique identifier of the rollout to undo",
          "pattern": "^[a-z0-9\\-_]+$"
        }
      },
      "required": [
        "rolloutSlug"
      ],
      "description": "Undo an add-on rollout"
    }
  }
}