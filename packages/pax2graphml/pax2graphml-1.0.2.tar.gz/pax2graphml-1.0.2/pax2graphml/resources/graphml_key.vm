<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

<!-- property keys -->

  <key id="key0" for="node" attr.name="biopaxType" attr.type="string" />
  <key id="key1" for="node" attr.name="color" attr.type="string" />
  <key id="key2" for="edge" attr.name="debug" attr.type="string" />
  <key id="key3" for="node" attr.name="description" attr.type="string" />
  <key id="key4" for="node" attr.name="input" attr.type="long" />
  <key id="key5" for="edge" attr.name="interaction" attr.type="string" />
  <key id="key6" for="node" attr.name="name" attr.type="string" />
  <key id="key7" for="node" attr.name="pk" attr.type="string" />
  <key id="key8" for="node" attr.name="shape" attr.type="string" />
  <key id="key9" for="edge" attr.name="spaim" attr.type="string" />

 <key id="key10" for="node" attr.name="simpleuri" attr.type="string" />
 <key id="key11" for="node" attr.name="spaimCase" attr.type="string" />
 <key id="key12" for="node" attr.name="patternTag" attr.type="string" />

  <graph id="$graphname" edgedefault="directed" parse.nodeids="free" parse.edgeids="free" parse.order="nodesfirst">

<!-- graph properties -->

<!-- vertices -->


#foreach( $n in $nodes )
  #if($n.export==true)
        $n.defaultVal()
        
        <node id="$n.numid">

           <data key="key0">$n.biopaxType</data>
           <data key="key1">$n.color</data>
	       <data key="key3">$n.safeDescription</data>
	       <data key="key4">$n.input</data>
	       <data key="key6">$n.safeLabel</data>
	       <data key="key8">$n.shape</data>
	       <data key="key10">$n.simpleuri</data>
	       <data key="key11">$n.spaimCaseFlat</data>
	       
##<data key="key12">$n.patternTagFlat</data>
##<data key="key7">$n.pk</data>

 
        </node>
  #end	
#end
   
<!-- edges -->
                   
 

#foreach( $e in $edges )
  #if($e.export==true)
        $e.defaultVal() 
        <edge id=""  source="$e.source" target="$e.target">
          
           ## <data key="SUID">$e.SUID</data>
           ## <data key="shared name">$e.shared_name</data>
           ## <data key="shared interaction">$e.shared_interactione</data>
           ## <data key="selected">$e.selected</data>
           ## <data key="name">$e.name</data> 

            <data key="key9">$e.spaim</data>
            ##<data key="key2">$e.debug</data>
            <data key="key5">$e.interaction</data>
        </edge>
	
  #end
#end

 



    </graph>
</graphml>

