	<key attr.name="pk" attr.type="string" for="node" id="pk"/>
	<key attr.name="spaim" attr.type="string" for="edge" id="spaim"/>
	
    <graph edgedefault="directed" id="$graphname">

#foreach( $n in $nodes )
        <node id="$n.numid"><data key="name">$n.safeLabel</data> <data key="pk">$n.pk</data></node>
        
#end
        
 #foreach( $e in $edges )       
        <edge source="$e.source" target="$e.target"><data key="spaim">$e.spaim</data></edge>
#end        
        
    </graph>
</graphml>