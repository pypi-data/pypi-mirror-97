/* QFrame --------------------------------------------------------------

--------------------------------------------------------------------------- */

.QFrame {
  border-radius: 4px;
  border: 1px solid #32414B;
}

.QFrame[frameShape=\"0\"] {
  border-radius: 4px;
  border: 1px transparent #32414B;
}

.QFrame[frameShape=\"4\"] {
  max-height: 2px;
  border: none;
  background-color: #32414B;
}

.QFrame[frameShape=\"5\"] {
  max-width: 2px;
  border: none;
  background-color: #32414B;
}

